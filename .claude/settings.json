{
  "mcp_servers": {
    "n8n-local": {
      "url": "http://localhost:5678/mcp-server/http",
      "description": "n8n workflow automation MCP server",
      "capabilities": ["workflow_execute", "workflow_list", "node_info"]
    },
    "spawner-skills": {
      "command": "npx github:vibeforge1111/vibeship-spawner-skills serve",
      "description": "462 production-grade skills MCP server"
    }
  },
  "external_skills": {
    "vibeship-spawner": {
      "repo": "github:vibeforge1111/vibeship-spawner-skills",
      "install": "npx github:vibeforge1111/vibeship-spawner-skills install --mcp",
      "skill_count": 462,
      "categories": 35
    }
  },
  "hooks": {
    "session_start": ".claude/hooks/session-start.sh",
    "pre_commit": null,
    "post_commit": null
  },
  "plugins": {
    "recommended": [
      "code-simplifier",
      "frontend-design",
      "commit-commands",
      "security-guidance",
      "feature-dev"
    ],
    "install_commands": [
      "/plugin marketplace add anthropics/claude-code",
      "/plugin install code-simplifier",
      "/plugin install frontend-design@claude-code-plugins"
    ]
  },
  "permissions": {
    "allow": [
      "Read(n8n-templates/**)",
      "Read(index.json)",
      "Read(.claude/**)",
      "Glob(**/*.json)",
      "Grep(**)",
      "Bash(cat:*)",
      "Bash(ls:*)",
      "Bash(git status)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(jq:*)"
    ],
    "ask": [
      "Write(n8n-templates/**)",
      "Edit(n8n-templates/**)",
      "Edit(index.json)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)"
    ],
    "deny": [
      "Read(.env*)",
      "Read(**/credentials*)",
      "Read(**/secrets*)",
      "Bash(rm -rf:*)",
      "Bash(curl:*POST*)",
      "Bash(wget:*)"
    ]
  },
  "preferences": {
    "output_style": "explanatory",
    "language": "turkish",
    "commit_style": "conventional"
  }
}
